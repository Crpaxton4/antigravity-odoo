[run]
branch = True
source = .
omit =
    */__init__.py
    */tests/*
    */migrations/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    ^\s*if\s+__name__\s*==\s*["\']__main__["\']\s*:\s*$

